<?xml version="1.0" encoding="ISO-8859-1"?>
<?xml-stylesheet type="text/xsl" href="../../help.xsl"?>
<documentation>
    <name>Data Entry</name>
    <icon>readfile.png</icon>
    <tags> 
        <tag>Data Input</tag> 
    </tags>
    <screenshots></screenshots>
    <summary>A table input data entry into a data.frame.</summary>
    <details><p>Similar to Excel, you can enter data into a table. A data.frame is created in R on submit and sent forward.</p></details>
    <relatedWidgets>
        <inputWidget><!-- <package>:<widget> ie. base:apply, plotting:heatmap --></inputWidget>
        <outputWidget><!-- <package>:<widget> ie. base:apply, plotting:heatmap --></outputWidget>
    </relatedWidgets>
    <signals>
    <!-- [REQUIRED] List all the widget input output slots and their data type.-->
        <input>
            <signalClass>None</signalClass>
            <description><!-- signal description --></description>
        </input>
        <output>
            <signalClass>base:RDataFrame</signalClass>
            <description><!-- signal description --></description>
        </output>
    </signals>
    <GUIElements>
        <!-- [REQUIRED] A list of the parameters and how to use them.-->
        <parameter>
            <name>Data Table</name>
            <description>input data as needed.</description>
        </parameter>
        <parameter>
            <name>Add column</name>
            <description>add additional columns of data input. On click requires input of column name.</description>
        </parameter>
    </GUIElements>
    <RFunctions>
    <!-- [REQUIRED] R functions used in this widget.-->
        <function>base:data.frame</function>
    </RFunctions>
    <citation>
    <!-- [REQUIRED] -->
        <author>
            <name>Red-R Core Team</name>
            <contact>http://www.red-r.org/contact</contact>
        </author>
        <reference>http://www.red-r.org</reference>
    </citation>
</documentation>